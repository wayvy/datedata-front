{
  "name": "core",
  "description": "DateData Core app",
  "version": "0.0.2",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:localserver": "VITE_API_URL=https://localhost:8000/api vite",
    "dev:localserver:host": "VITE_API_URL=https://localhost:8000/api vite --host",
    "build": "tsc -b && vite build",
    "preview": "yarn build && vite preview",
    "eslint": "eslint \"**/*.js\" \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "eslint:fix": "eslint --fix \"**/*.js\" \"src/**/*.{ts,tsx}\"",
    "stylelint": "stylelint \"src/**/*.scss\" --max-warnings 0",
    "stylelint:fix": "stylelint --fix \"src/**/*.scss\"",
    "prettier": "prettier --check \"**/*.{js,ts,tsx,scss}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts,tsx,scss}\"",
    "ts-check": "tsc --noEmit -p tsconfig.json",
    "codestyle": "yarn ts-check && yarn eslint && yarn stylelint && yarn prettier",
    "codestyle:fix": "yarn ts-check && yarn eslint:fix && yarn stylelint:fix && yarn prettier:fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@gravity-ui/date-components": "^3.2.3",
    "@gravity-ui/date-utils": "^2.5.6",
    "@gravity-ui/icons": "^2.13.0",
    "@gravity-ui/navigation": "^3.3.9",
    "@gravity-ui/uikit": "^7.16.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "maplibre-gl": "^5.6.1",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "re-resizable": "^6.11.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-map-gl": "^8.0.4",
    "react-router": "^7.6.2",
    "swiper": "^11.2.8",
    "temporal-polyfill": "^0.3.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/models": "*",
    "@repo/stores": "*",
    "@repo/stylelint-config": "*",
    "@repo/types": "*",
    "@repo/typescript-config": "*",
    "@repo/ui": "*",
    "@repo/utils": "*",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^9.29.0",
    "postcss": "^8.5.5",
    "prettier": "^3.5.3",
    "stylelint": "^16.20.0",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-compression2": "^2.2.0"
  }
}
